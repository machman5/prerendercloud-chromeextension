{
"name": "Prerender.cloud - disables Javascript locally, renders it remotely",
"version": "0.0.1",
"manifest_version": 2,
"description": "Surf the Javascript information superhighway without Javascript",
"homepage_url": "https://www.prerender.cloud/documentation/chrome-extension",
"permissions": [
  "webNavigation",
  "contentSettings",
  "webRequestBlocking",
  "webRequest",
  "http://*/*",
  "https://*/*",
  "tabs",
  "storage"
],
"icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
"background": {
  "scripts": ["background.js"],
  "persistent": true
},
"browser_action": {
  "default_popup": "browser_action.html"
},
"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
